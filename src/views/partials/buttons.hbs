{{#if tag}}

<{{ tag }} 
    href="{{href}}"
    method="{{method}}"
    class="btn btn-{{type}} {{ class }}">

    {{ message }}

</{{ tag}}>

{{ else }}
<button
    class="btn btn-{{type}} {{class }}"
    type="{{ nativeType }}">
    {{ message }}
</button>

{{/if}}